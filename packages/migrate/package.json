{
  "name": "@nestia/migrate",
  "version": "0.13.16",
  "description": "Migration program from swagger to NestJS",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "@nestia/migrate": "lib/executable/migrate.js"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "bundle": "ts-node src/executable/bundle.ts",
    "dev": "npm run build -- --watch",
    "package:next": "npm publish --access public --tag next",
    "prepare": "ts-patch install && typia patch && npm run bundle",
    "test": "node lib/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/nestia"
  },
  "keywords": [
    "migration",
    "swagger",
    "openapi generator",
    "NestJS",
    "nestia",
    "SDK",
    "RPC",
    "Mockup Simulator"
  ],
  "author": "Jeongho Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/nestia/issues"
  },
  "homepage": "https://nestia.io",
  "devDependencies": {
    "@nestia/core": "^3.2.0",
    "@nestia/e2e": "^0.4.3",
    "@nestia/fetcher": "^3.2.0",
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.8",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/platform-fastify": "^10.3.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.3.3",
    "@types/swagger-ui-express": "^4.1.6",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "express": "^4.19.2",
    "rimraf": "^5.0.1",
    "serialize-error": "^4.1.0",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.1.0",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@samchon/openapi": "^0.1.22",
    "commander": "10.0.0",
    "inquirer": "8.2.5",
    "prettier": "^3.2.5",
    "tstl": "^3.0.0",
    "typescript": "^5.4.5",
    "typia": "^6.0.5"
  },
  "files": [
    "lib",
    "src",
    "!lib/test",
    "!src/test",
    "package.json",
    "README.md",
    "LICENSE"
  ]
}
